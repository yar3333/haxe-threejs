ID = [_a-zA-Z][_a-zA-Z0-9]*
TYPE = ID(?:[<]\s*ID(?:\s*,\s*ID)*\s*[>])?
TYPE_COMPLEX = (?:TYPE|\((?:\s*ID\s*[:]\s*TYPE(?:,\s*ID\s*[:]\s*TYPE)*)?\)\s*[=][>]\s*TYPE)

/\bexport\s+(?:var\s+)?(ID)\s*:\s*\{/extern class $1\n{/
/(\n[ \t]*)(ID)\s*:\s*(TYPE_COMPLEX)[;,]/$1static var $2 : $3;/
/(\n[ \t]*)(ID)\s*:\s*(TYPE_COMPLEX)([ \t\n]*\})/$1static var $2 : $3;$4/
/(\n[ \t]*)function\b/$1static function/

/Triangulate\s*:\s*\{[ \t\n]*(.*?)\n[ \t]*static\s+function\s+area[^;]+;\s*\}/static function Triangulate$1/s
/faces\s*:\s*\{\s*\[\s*weight\s*:\s*String\s*\]\s*:\s*Dynamic<Face3>\s*\}/static var faces : Dynamic<Face3>/

/(class GeometryUtils\b.*?)(\n[ \t]+)(static function merge)/$1$2\/\/$3/s


